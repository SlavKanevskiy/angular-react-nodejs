generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Location {
  id   Int     @id @default(autoincrement())
  name String  @db.VarChar(255)
  lat  Decimal @db.Decimal(10, 8)
  lon  Decimal @db.Decimal(11, 8)

  @@map("locations")
}

